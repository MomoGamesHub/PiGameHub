{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { 
          "key": "Content-Security-Policy", 
          "value": "frame-ancestors 'self' https://*.pinet.com https://*.pi-blockchain.net https://*.minepi.com https://minepi.com;" 
        }
      ]
    }
  ]
}
